{
  "name": "n8n-nodes-myob",
  "version": "0.1.0",
  "description": "n8n node for MYOB AccountRight",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "tsc --watch"
  },
  "n8n": {
    "nodes": [
      {
        "type": "node",
        "file": "dist/nodes/Myob.node.js"
      }
    ],
    "credentials": [
      {
        "type": "credentials",
        "file": "dist/credentials/MyobOAuth2Api.credentials.js"
      },
      {
        "type": "credentials",
        "file": "dist/credentials/MyobCompanyFile.credentials.js"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "n8n-workflow": "^1.82.0"
  }
}
